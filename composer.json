{
    "name": "javiersolis/laravel-env-set-command",
    "description": "FORK:Set a .env file variable from the command line",
    "keywords": [
        "imliam",
        "laravel-env-set-command",
        "environment",
        "variables",
        "env",
        "artisan"
    ],
    "homepage": "https://github.com/JavierSolis/laravel-env-set-command",
    "license": "MIT",
    "authors": [
        {
            "name": "Liam Hammett",
            "email": "liam@liamhammett.com",
            "homepage": "https://liamhammett.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.36.3",
        "illuminate/console": "^9.36.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "ImLiam\\EnvironmentSetCommand\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ImLiam\\EnvironmentSetCommand\\EnvironmentSetCommandServiceProvider"
            ]
        }
    }
}
